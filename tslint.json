{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [false, "always-prefix"],
    "ordered-imports": false,
    "member-access": [true, "no-public"],
    "jsx-no-lambda": false,
    "member-ordering": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "no-console": [false]
  }
}
